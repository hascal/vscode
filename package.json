{
  "name": "sacalon",
  "displayName": "Sacalon Language",
  "description": " Sacalon is a general purpose and open source programming language designed to build optimal, maintainable, reliable and efficient software. ",
  "version": "0.1.1",
  "icon": "logo.jpeg",
  "publisher": "SacalonFoundation",
  "repository": {
    "url": "https://github.com/sacalon/vscode"
  },
  "engines": {
    "vscode": "^1.5.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [
      {
        "id": "sacalon",
        "aliases": [
          "Sacalon",
          "sacalon"
        ],
        "extensions": [
          ".sa"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "sacalon",
        "scopeName": "source.sa",
        "path": "./syntaxes/sacalon.tmLanguage.json"
      }
    ]
  }
}
